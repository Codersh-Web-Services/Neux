<section class="makers">
    <div class="container">
        <div class="row text-center">
            <div class="col-12">
                <h1>{{ section.settings.pagetitle  | escape }}</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="makers-content">
                    <div class="img-wrap">
                        {%- assign img_url = section.settings.image | image_url -%}
                        <img src="{{ img_url }}" alt="{{ section.settings.image.alt | escape }}">
                    </div>
                    <h3 class="mt-3">
                        {{ section.settings.header  | escape }}
                    </h3>
                    <ul>
                        {% for block in section.blocks %}
                          {% case block.type %}
                            {% when 'list' %}
                            <li>
                                <span>
                                    <svg xmlns="http://www.w3.org/2000/svg" version="1.2" viewBox="0 0 49 49" width="49" height="49">
                                        <title>Layer 1</title>
                                        <style>
                                            .line {fill: none;
                                                stroke: #000; } 
                                        </style>
                                        <g id="Layer 1">
                                            <g id="&lt;Group&gt;">
                                                <g id="&lt;Group&gt;">
                                                    <g id="-e-">
                                                        <path id="&lt;Path&gt;" class="line" d="m39.4 28.6c-5 2-9 5.9-10.9 10.9l-3.7 9.4c-0.1 0.2-0.5 0.2-0.6 0l-3.6-9.4c-1.9-5-5.9-8.9-10.9-10.9l-9.3-3.6c-0.3-0.1-0.3-0.5 0-0.6l9.3-3.6c5-2 9-5.9 10.9-10.9l3.6-9.4c0.1-0.2 0.5-0.2 0.6 0l3.7 9.4c1.9 5 5.9 8.9 10.9 10.9"/>
                                                        <path id="&lt;Path&gt;" class="line" d="m39.4 20.8l9.3 3.6c0.3 0.1 0.3 0.5 0 0.6l-9.3 3.6"/>
                                                    </g>
                                                </g>
                                            </g>
                                        </g>
                                    </svg>
                                </span>
                                <p class="mb-0">
                                    {{ block.settings.aboutdetails }}
                                </p>
                            </li>
                          {% endcase %}
                        {% endfor %}
                    </ul>
                </div>
            </div>
            <div class="col-md-6">
                <div class="makers-content">
                    <p>
                        {{ section.settings.headerbusiness  | escape }}
                    </p>
                    <div class="img-wrap">
                        {%- assign img_url = section.settings.imagebusiness | image_url -%}
                        <img src="{{ img_url }}" alt="{{ section.settings.imagebusiness.alt | escape }}">
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

