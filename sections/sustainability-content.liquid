<section class="sustainbility">
    <div class="container">
        <div class="row">
            <div class="col-12 text-center">
                <h2>{{ section.settings.headersustainbility  | escape }}</h2>
            </div>
            <div class="col-md-5">
                <div class="img-wrap">
                    {%- assign img_url = section.settings.imagesustainbility | image_url -%}
                    <img src="{{ img_url }}" alt="{{ section.settings.imagesustainbility.alt | escape }}">
                </div>
            </div>
            <div class="col-md-7">
                <p class="my-5">
                    {{ section.settings.sustainbilitydescription  | escape }}
                </p>
                <h3>{{ section.settings.sowhatheader  | escape }}</h3>
                <ul>
                    {% for block in section.blocks %}
                      {% case block.type %}
                        {% when 'list-2' %}
                        <li>
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" version="1.2" viewBox="0 0 49 49" width="49" height="49">
                                    <title>Layer 1</title>
                                    <style>
                                        .line {fill: none;
                                            stroke: #000; } 
                                    </style>
                                    <g id="Layer 1">
                                        <g id="&lt;Group&gt;">
                                            <g id="&lt;Group&gt;">
                                                <g id="-e-">
                                                    <path id="&lt;Path&gt;" class="line" d="m39.4 28.6c-5 2-9 5.9-10.9 10.9l-3.7 9.4c-0.1 0.2-0.5 0.2-0.6 0l-3.6-9.4c-1.9-5-5.9-8.9-10.9-10.9l-9.3-3.6c-0.3-0.1-0.3-0.5 0-0.6l9.3-3.6c5-2 9-5.9 10.9-10.9l3.6-9.4c0.1-0.2 0.5-0.2 0.6 0l3.7 9.4c1.9 5 5.9 8.9 10.9 10.9"/>
                                                    <path id="&lt;Path&gt;" class="line" d="m39.4 20.8l9.3 3.6c0.3 0.1 0.3 0.5 0 0.6l-9.3 3.6"/>
                                                </g>
                                            </g>
                                        </g>
                                    </g>
                                </svg>
                            </span>
                            <p class="mb-0">
                                {{ block.settings.sowhatdetails }}
                            </p>
                        </li>
                      {% endcase %}
                    {% endfor %}
                </ul>
            </div>
        </div>
        <div class="row justify-content-center mt-5">
            <div class="col-md-8 text-center">
                <p>
                    {{ section.settings.sodescription  | escape }}
                </p>
            </div>
        </div>
    </div>
</section>

    
{% schema %}
{
    "name": "sustainability page",
    "settings":[ 
        {
        "type": "text",
        "id": "pagetitle",
        "label": "Page Title"
        },
        {
        "type": "image_picker",
        "id": "image",
        "label": "Image"
        },
        {
        "type": "text",
        "id": "header",
        "label": "Header"
        },
        {
        "type": "image_picker",
        "id": "imagebusiness",
        "label": "ImageBusiness"
        },
        {
        "type": "textarea",
        "id": "headerbusiness",
        "label": "HeaderBusiness"
        },
        {
        "type": "textarea",
        "id": "feedback",
        "label": "Feedback"
        },
        {
        "type": "image_picker",
        "id": "boximage",
        "label": "Box Image"
        },
        {
        "type": "text",
        "id": "headersustainbility",
        "label": "Sustainbility Header"
        },
        {
        "type": "image_picker",
        "id": "imagesustainbility",
        "label": "Image Sustainbility"
        },
        {
        "type": "textarea",
        "id": "sustainbilitydescription",
        "label": "Sustainbility Description"
        },
        {
        "type": "text",
        "id": "sowhatheader",
        "label": "So What Header"
        },
        {
        "type": "textarea",
        "id": "sodescription",
        "label": "So Description"
        }
    ],

    "blocks": [
        {
           "name": "About Details",
           "type": "list",
           "settings": [
            {
               "type": "text",
               "id": "aboutdetails",
               "label": "About Details"
            }
           ]
        },
        {
           "name": "So What Details",
           "type": "list-2",
           "settings": [
            {
               "type": "text",
               "id": "sowhatdetails",
               "label": "So What Details"
            }
           ]
        }
    ],


    "presets": [
        {
            "name": "sustainability page",
            "category": "Page"
        }
    ]
}
{% endschema %}