      "about-us-content": {
        "type": "about-us-content",
        "blocks": {
          "0bc85dc0-f6e1-429e-9157-1ac5ea1fa275": {
            "type": "list",
            "settings": {
              "aboutdetails": "Considered design choices for different body types (adjustable straps, stretchy fabrics, bust cups for support)"
            }
          },
          "5a0f292f-7c7e-45ad-a3c6-b1675d709adb": {
            "type": "list",
            "settings": {
              "aboutdetails": "We appreciate you as a consumer. We want you to feel confident and empowered in your Neux garments. You are not just a number to us. We welcome and appreciate any feedback."
            }
          },
          "7160fe97-bf93-43bd-ada2-fa07a1b8b51b": {
            "type": "list",
            "settings": {
              "aboutdetails": "We offer sizes 6-16. We aim to increase this size range as our brand grows."
            }
          },
          "01fabe25-2b47-4dc5-b9cf-13dd6013075e": {
            "type": "list",
            "settings": {
              "aboutdetails": "We pledge to always be a transparent, ethical, and sustainable brand."
            }
          },
          "c979a32a-34a0-4675-bd12-19d47c30b7d1": {
            "type": "list",
            "settings": {
              "aboutdetails": "It is important to us that women feel represented, seen, and heard."
            }
          }
        },
        "block_order": [
          "0bc85dc0-f6e1-429e-9157-1ac5ea1fa275",
