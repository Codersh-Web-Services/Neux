          },
          "333b48e8-b447-4f70-a909-86398152eb8e": {
            "type": "list-2",
            "settings": {
              "sowhatdetails": "Preference the use of natural fibres such as rayons, silks and cottons where feasable."
            }
          },
          "d7fd4894-52b2-4bc5-8a54-5db70e3c4416": {
            "type": "list-2",
            "settings": {
              "sowhatdetails": "High quality finishings such as french seaming and pre-shrinking fabric to ensure longevity of garment life. We encourage you to let your friends borrow, share and re-sell garments."
            }
          },
          "de4e7c0b-3221-449b-830d-5dc21ca3201c": {
